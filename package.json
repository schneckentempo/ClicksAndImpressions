{
  "name": "react-hot-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS project with hot code reloading",
  "scripts": {
    "start": "node server.js",
    "lint": "./node_modules/eslint/bin/eslint src",
    "test": "./node_modules/mocha/bin/mocha \"src/**/test/*.spec.js\" --recursive --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-hot-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Dan Abramov <dan.abramov@me.com> (http://github.com/gaearon)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gaearon/react-hot-boilerplate/issues"
  },
  "homepage": "https://github.com/gaearon/react-hot-boilerplate",
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.0",
    "enzyme": "^2.5.1",
    "eslint": "3.5.0",
    "eslint-config-airbnb": "11.1.0",
    "eslint-config-airbnb-base": "7.1.0",
    "eslint-import-resolver-webpack": "0.6.0",
    "eslint-plugin-import": "1.15.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.2.2",
    "jsdom": "9.8.3",
    "jsdom-global": "2.1.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.2.0",
    "mocha": "^3.1.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.1",
    "react-hot-loader": "^1.3.0",
    "sinon": "^1.17.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "axios": "^0.13.1",
    "lodash": "^4.13.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-select": "^1.0.0-rc.2"
  }
